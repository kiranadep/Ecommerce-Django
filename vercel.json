{
  "version": 2,
  "builds": [
    {
      "src": "E_shop/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime":"python3.10"
      },
      {
      "src": "bulid_files.sh",
      "use":"@vercel/static-build",
        "config": {
          "distDir":"staticfiles_build"
      },
      }
    }
  ],
  "routes": [
    {
      "src":"/static/(.*)",
      "dest":"/static/$1"
    }
    {
      "src": "/(.*)",
      "dest": "E_shop/wsgi.py"
    }
  ]
}
